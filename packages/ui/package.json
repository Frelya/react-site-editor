{
    "name": "@react-site-editor/ui",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "main": "./src/index.ts",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "start": "vite",
        "scripts:init": "find ./scripts/ -type f -iname \"*.sh\" -exec chmod +x {} \\;",
        "components:expose": "pnpm scripts:init && ./scripts/expose-components.sh",
        "components:create": "pnpm scripts:init && ./scripts/create-component.sh",
        "storybook:dev": "storybook dev -p 3000 --ci --disable-telemetry",
        "storybook:dev:docs": "pnpm storybook:dev --docs",
        "storybook:dev:comp": "pnpm storybook:dev -- --component",
        "storybook:dev:docs:comp": "pnpm storybook:dev:docs -- --component",
        "storybook:build": "storybook build"
    },
    "dependencies": {
        "@react-site-editor/functions": "workspace:*",
        "@react-site-editor/types": "workspace:*",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@storybook/addon-essentials": "7.5.1",
        "@storybook/addon-interactions": "7.5.1",
        "@storybook/addon-links": "7.5.1",
        "@storybook/blocks": "7.5.1",
        "@storybook/builder-vite": "7.5.1",
        "@storybook/manager-api": "7.5.1",
        "@storybook/react": "7.5.1",
        "@storybook/react-vite": "7.5.1",
        "@storybook/types": "7.5.1",
        "@types/glob": "8.1.0",
        "@types/react": "18.2.31",
        "@types/react-dom": "18.2.14",
        "@vitejs/plugin-react": "3.1.0",
        "autoprefixer": "10.4.16",
        "esbuild": "0.17.19",
        "fast-glob": "3.3.1",
        "postcss": "8.4.31",
        "prop-types": "15.8.1",
        "storybook": "7.5.1",
        "tailwindcss": "3.3.3",
        "vite": "4.5.0"
    }
}
