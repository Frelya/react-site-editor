{
    "name": "@react-site-editor/ui",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "main": "./dist/rse.esm.js",
    "scripts": {
        "dev": "vite",
        "pre-build": "chmod +x ./scripts/build.sh && ./scripts/build.sh --external:react --external:react-dom",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "start": "npm run build && vite",
        "init:scripts": "find ./scripts/ -type f -iname \"*.sh\" -exec chmod +x {} \\;",
        "components:expose": "npm run init:scripts && ./scripts/expose-components.sh",
        "components:create": "npm run init:scripts && ./scripts/create-component.sh"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/glob": "^8.1.0",
        "@types/postcss-js": "^4.0.0",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "@vitejs/plugin-react": "^3.1.0",
        "autoprefixer": "^10.4.13",
        "esbuild": "^0.17.8",
        "fast-glob": "^3.2.11",
        "postcss": "^8.4.21",
        "prettier": "2.8.7",
        "tailwindcss": "^3.2.6",
        "typescript": "^4.9.3",
        "vite": "^4.1.0"
    }
}