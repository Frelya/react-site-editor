{
    "name": "@react-site-editor/backend",
    "version": "0.0.1",
    "private": true,
    "description": "React Site Editor website backend",
    "scripts": {
        "build": "pnpm docs:generate && nest build",
        "start": "pnpm docs:generate && nest start",
        "dev": "pnpm docs:generate && nest start --watch",
        "db:push": "prisma db push",
        "db:seed": "prisma db seed",
        "docs:generate": "prisma generate && pnpm docs:rename",
        "docs:rename": "chmod +x ./scripts/retitle-docs.sh && ./scripts/retitle-docs.sh"
    },
    "dependencies": {
        "@nestjs/common": "10.2.7",
        "@nestjs/config": "3.1.1",
        "@nestjs/core": "10.2.7",
        "@nestjs/jwt": "10.1.1",
        "@nestjs/platform-express": "10.2.7",
        "@nestjs/serve-static": "4.0.0",
        "@prisma/client": "5.4.2",
        "@react-site-editor/types": "workspace:*",
        "bcrypt": "5.1.1",
        "bson": "5.5.1",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.0",
        "cookie-parser": "1.4.6",
        "helmet": "7.0.0",
        "nest-winston": "1.9.4",
        "prisma": "5.4.2",
        "prisma-docs-generator": "0.8.0",
        "reflect-metadata": "0.1.13",
        "response-time": "2.3.2",
        "rxjs": "7.8.1",
        "source-map-support": "0.5.21",
        "winston": "3.11.0",
        "zod": "3.22.4"
    },
    "devDependencies": {
        "@nestjs/cli": "10.2.0",
        "@nestjs/schematics": "10.0.2",
        "@types/bcrypt": "5.0.1",
        "@types/cookie-parser": "1.4.5",
        "@types/express": "4.17.20",
        "@types/module-alias": "2.0.3",
        "@types/response-time": "2.3.7",
        "ts-loader": "9.5.0",
        "ts-node": "10.9.1"
    },
    "prisma": {
        "seed": "ts-node ./scripts/seed/seed.ts"
    }
}
