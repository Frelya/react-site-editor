{
    "name": "react-site-editor",
    "version": "0.0.1",
    "private": true,
    "author": "Frelya",
    "license": "MIT",
    "engines": {
        "node": ">=16.19.0"
    },
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "dev": "npm-run-all --parallel backend:dev frontend:dev",
        "start": "npm-run-all --parallel backend:start frontend:start",
        "backend": "pnpm --filter backend",
        "backend:build": "pnpm backend build",
        "backend:dev": "pnpm backend dev",
        "backend:start": "pnpm backend start",
        "backend:docs": "pnpm backend db:generate",
        "frontend": "pnpm --filter frontend",
        "frontend:build": "pnpm frontend build",
        "frontend:dev": "pnpm frontend dev",
        "frontend:start": "pnpm frontend start",
        "frontend:icons": "pnpm frontend icons",
        "frontend:preview": "pnpm frontend preview",
        "frontend:scripts:init": "pnpm frontend scripts:init",
        "ui": "pnpm --filter ui",
        "ui:build": "pnpm ui build",
        "ui:components": "pnpm ui storybook:dev",
        "ui:components:create": "pnpm ui components:create",
        "ui:components:expose": "pnpm ui components:expose",
        "ui:dev": "pnpm ui dev",
        "ui:docs": "pnpm ui storybook:dev:docs",
        "ui:preview": "pnpm ui preview",
        "ui:scripts:init": "pnpm ui scripts:init",
        "ui:start": "pnpm ui start",
        "lint": "npx eslint --cache --ext .js,.ts,.jsx,.tsx,.json .",
        "lint:fix": "pnpm lint --fix",
        "format": "npx prettier --cache --write ./ '!**/*.{js,ts,jsx,tsx,html,json}'",
        "ci:style": "pnpm lint:fix && pnpm format",
        "ci:all": "pnpm ci:style && pnpm test",
        "push": "pnpm ci:all && git commit -am 'ci: lint and format' && git push"
    },
    "devDependencies": {
        "@types/node": "20.8.8",
        "@types/prettier": "2.7.3",
        "@typescript-eslint/eslint-plugin": "5.62.0",
        "@typescript-eslint/parser": "5.62.0",
        "eslint": "8.52.0",
        "eslint-config-prettier": "8.10.0",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "7.33.2",
        "npm-run-all": "4.1.5",
        "prettier": "2.8.3",
        "typescript": "4.9.5"
    },
    "packageManager": "pnpm@8.5.1",
    "dependencies": {
        "dotenv": "16.3.1"
    }
}
