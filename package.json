{
    "name": "react-site-editor",
    "version": "0.0.1",
    "private": true,
    "author": "Frelya",
    "license": "MIT",
    "engines": {
        "node": ">=16.19.0",
        "pnpm": ">=8.5.1"
    },
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "dev": "pnpm frontend:dev",
        "start": "pnpm frontend:start",
        "build": "turbo run build",
        "test": "turbo run test",
        "frontend:build": "pnpm --filter frontend build",
        "frontend:dev": "pnpm --filter frontend dev",
        "frontend:icons": "pnpm --filter frontend icons",
        "frontend:preview": "pnpm --filter frontend preview",
        "frontend:scripts:init": "pnpm --filter frontend scripts:init",
        "frontend:start": "pnpm --filter frontend start",
        "ui:build": "pnpm --filter ui build",
        "ui:components:create": "pnpm --filter ui components:create",
        "ui:components:expose": "pnpm --filter ui components:expose",
        "ui:dev": "pnpm --filter ui dev",
        "ui:preview": "pnpm --filter ui preview",
        "ui:scripts:init": "pnpm --filter ui scripts:init",
        "ui:start": "pnpm --filter ui start",
        "lint": "npx eslint --cache --ext .js,.ts,.jsx,.tsx,.json .",
        "lint:fix": "pnpm lint --fix",
        "format": "npx prettier --cache --write ./ '!**/*.{js,ts,jsx,tsx,html,json}'",
        "ci:style": "pnpm lint:fix && pnpm format",
        "ci:all": "pnpm ci:style && pnpm test",
        "push": "pnpm ci:all && git commit -am 'ci: lint and format' && git push"
    },
    "devDependencies": {
        "@types/node": "^18.13.0",
        "@types/prettier": "^2.7.2",
        "@typescript-eslint/eslint-plugin": "^5.50.0",
        "@typescript-eslint/parser": "^5.50.0",
        "eslint": "^8.33.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "prettier": "2.8.3",
        "turbo": "^1.9.6"
    },
    "packageManager": "pnpm@8.5.1"
}
